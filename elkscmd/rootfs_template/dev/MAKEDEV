#!/bin/sh

MKDEV=/bin/mknod

# Devices are generated by this script in the same order that they are
# listed in the Documentation/devices.txt file.

# Memory devices.

	$MKDEV mem	c 1 1
	$MKDEV kmem	c 1 2
	$MKDEV null	c 1 3
#	$MKDEV port	c 1 4
	$MKDEV zero	c 1 5
	$MKDEV full	c 1 7
#	$MKDEV random	c 1 8
#	$MKDEV urandom	c 1 9
#	$MKDEV aio	c 1 10

# RAM disks.

	$MKDEV ram0	b 1 0
	$MKDEV ram1	b 1 1
	$MKDEV ram2	b 1 2
	$MKDEV ram3	b 1 3
	$MKDEV ram4	b 1 4
	$MKDEV ram5	b 1 5
	$MKDEV ram6	b 1 6
	$MKDEV ram7	b 1 7

# BIOS hard disks. Only primary partitions are currently enabled.

	$MKDEV bda	b 3 0
	$MKDEV bda1	b 3 1
	$MKDEV bda2	b 3 2
	$MKDEV bda3	b 3 3
	$MKDEV bda4	b 3 4
#	$MKDEV bda5	b 3 5
#	$MKDEV bda6	b 3 6
#	$MKDEV bda7	b 3 7

	$MKDEV bdb	b 3 64
	$MKDEV bdb1	b 3 65
	$MKDEV bdb2	b 3 66
	$MKDEV bdb3	b 3 67
	$MKDEV bdb4	b 3 68
#	$MKDEV bdb5	b 3 69
#	$MKDEV bdb6	b 3 70
#	$MKDEV bdb7	b 3 71

# BIOS floppies. Only the autodetect nodes are listed.

	$MKDEV fd0	b 3 128

	$MKDEV fd1	b 3 192

# Virtual consoles. Note that tty4 through tty7 can't be enabled at this
# time because of the possible conflict with ttyS0 through ttyS3 which
# used to use the node numbers these now use. However, it is expected that
# this restriction can be removed in the near future.

	$MKDEV tty0	c 4 0
	$MKDEV tty1	c 4 1
	$MKDEV tty2	c 4 2
	$MKDEV tty3	c 4 3
#	$MKDEV tty4	c 4 4
#	$MKDEV tty5	c 4 5
#	$MKDEV tty6	c 4 6
#	$MKDEV tty7	c 4 7
#	$MKDEV tty8	c 4 8
#	$MKDEV tty9	c 4 9
#	$MKDEV tty10	c 4 10
#	$MKDEV tty11	c 4 11
#	$MKDEV tty12	c 4 12
#	$MKDEV tty13	c 4 13
#	$MKDEV tty14	c 4 14
#	$MKDEV tty15	c 4 15

# Serial ports. These moved from (4,4-7) to (4,64-67) to comply with the
# standard in the 0.1.0-pre4 kernel.

	$MKDEV ttyS0	c 4 64
	$MKDEV ttyS1	c 4 65
	$MKDEV ttyS2	c 4 66
	$MKDEV ttyS3	c 4 67

# Direct IDE disks. Only primary partitions are currently enabled.

	$MKDEV hda	b 5 0
	$MKDEV hda1	b 5 1
	$MKDEV hda2	b 5 2
	$MKDEV hda3	b 5 3
	$MKDEV hda4	b 5 4
#	$MKDEV hda5	b 5 5
#	$MKDEV hda6	b 5 6
#	$MKDEV hda7	b 5 7

	$MKDEV hdb	b 5 64
	$MKDEV hdb1	b 5 65
	$MKDEV hdb2	b 5 66
	$MKDEV hdb3	b 5 67
	$MKDEV hdb4	b 5 68
#	$MKDEV hdb5	b 5 69
#	$MKDEV hdb6	b 5 70
#	$MKDEV hdb7	b 5 71

	$MKDEV hdc	b 5 128
	$MKDEV hdc1	b 5 129
	$MKDEV hdc2	b 5 130
	$MKDEV hdc3	b 5 131
	$MKDEV hdc4	b 5 132
#	$MKDEV hdc5	b 5 133
#	$MKDEV hdc6	b 5 134
#	$MKDEV hdc7	b 5 135

	$MKDEV hdd	b 5 192
	$MKDEV hdd1	b 5 193
	$MKDEV hdd2	b 5 194
	$MKDEV hdd3	b 5 195
	$MKDEV hdd4	b 5 196
#	$MKDEV hdd5	b 5 197
#	$MKDEV hdd6	b 5 198
#	$MKDEV hdd7	b 5 199

# Alternate consoles. These are not yet supported.

#	$MKDEV tty	c 5 0
#	$MKDEV console	c 5 1

# Parallel devices.

	$MKDEV lp0	c 6 0
	$MKDEV lp1	c 6 1
	$MKDEV lp2	c 6 2

# Loopback devices. These are not yet supported.

#	$MKDEV loop0	b 7 0
#	$MKDEV loop1	b 7 1
#	$MKDEV loop2	b 7 2
#	$MKDEV loop3	b 7 3
#	$MKDEV loop4	b 7 4
#	$MKDEV loop5	b 7 5
#	$MKDEV loop6	b 7 6
#	$MKDEV loop7	b 7 7

# Z80-SIO devices. These are not yet supported.

#	$MKDEV scc0	c 34 0
#	$MKDEV scc1	c 34 1
#	$MKDEV scc2	c 34 2
#	$MKDEV scc3	c 34 3
#	$MKDEV scc4	c 34 4
#	$MKDEV scc5	c 34 5
#	$MKDEV scc6	c 34 6
#	$MKDEV scc7	c 34 7

# EOF.
